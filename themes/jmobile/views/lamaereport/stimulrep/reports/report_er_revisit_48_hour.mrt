      <DataSource1 type="Stimulsoft.Report.Dictionary.StiMySqlSource" isKey="true" Ref="3">
        <Alias>DataSource1</Alias>
        <Columns count="7" isList="true">
          <value>lastvisit_hour,System.Int32</value>
          <value>hn,System.String</value>
          <value>pt_name,System.String</value>
          <value>pdx,System.String</value>
          <value>icd_name,System.String</value>
          <value>thainame,System.String</value>
          <value>vstdate,System.DateTime</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1"/>
        <Name>DataSource1</Name>
        <NameInSource>Connection1</NameInSource>
        <Parameters count="0" isList="true"/>
        <SqlCommand>SELECT 

v.lastvisit_hour,v.hn,concat(p.pname,p.fname,'  ',p.lname) as pt_name,v.pdx ,i.name as icd_name ,i.tname as thainame,v.vstdate

FROM  vn_stat   v

LEFT OUTER JOIN patient p on p.hn =v.hn
LEFT OUTER JOIN icd101 i on i.code = v.pdx
RIGHT OUTER JOIN er_regist e  on e.vn = v.vn

WHERE 

v.vstdate BETWEEN "{datestart}"  AND  "{dateend}" 
AND v.lastvisit_hour &lt;=48 AND v.pdx  not in ("Z480","Z488")

ORDER BY  v.vstdate</SqlCommand>
      </DataSource1>
    </DataSources>
